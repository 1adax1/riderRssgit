{# templates/list/index.html.twig #} {% extends 'base.html.twig' %} {% block
body %}
<div class="container-fluid">
  <div class="row">
    <div class="col-md-12">
      <div class="card bg-light mb-3 mt-3">
        <div class="card-body">
          <div class="card-header">RSS лента</div>
          <AmpContent>
          <div class="row">
          <div class="col-md-6">
          <table class="table">
            <tr>
              <th>Title</th>
              <th>Text</th>
            </tr>
            {% for key, item in array_text_table['title'] %}
              {% if item %}
                <tr>
                  <td>{{ array_text_table['title'][key] }}</td>
                  <td>{{ array_text_table['summary'][key] }}</td>
                </tr>
              {% endif %}
            {% endfor %}
          </table>
          </div>
          <div class="col-md-6">
          <h1>Повторяющие слова в RSS ленте</h1>
          <div class="row">
            {% for key, item in array_text %}
              <div class="alert alert-danger m-1">
                {{ item['word'] }}
              </div>
            {% endfor %}
          </div>
          <div class="col-md-6">
          <h1>10 Повторяющих слов в RSS ленте</h1>
          <div class="row">
          {% for ten_word, ten_key in ten_array_text_table %}
            <div class="alert alert-success m-1">
              {{ ten_word }}
            </div>
          {% endfor %}
          </div>
          </div>
          </NonAmpContent>
        </div>
      </div>
    </div>
  </div>
</div>
{% endblock %}
